package array;

public class Array1 {

	public static void main(String[] args) {
		String [] carros  = {"ferrari", "fusca","camaro", "uno"};
		//√çndice do array 		[0]		   [1]	   [2]      [3]
		System.out.println("Tamanho do array: " + carros.length);
		System.out.println("Carros: " + carros[2]);
		carros[2] = "Porsche";
		System.out.println("Carros: " + carros[2]);
	}

}
